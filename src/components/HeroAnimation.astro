<style>
    /* STAGE SETUP */
    .animation-stage {
        position: relative;
        width: 380px; 
        height: 680px; 
        margin: 0 auto;
        perspective: 1500px;
        transform-style: preserve-3d;
        font-family: var(--font-ar);
    }

    /* SHARED CARD STYLES */
    .scene-card {
        position: absolute;
        background-color: white;
        background-repeat: no-repeat;
        border-radius: 24px;
        box-shadow: 0 25px 60px rgba(0,0,0,0.15);
        opacity: 0;
        overflow: hidden; /* Important for header clip */
        background-size: 100% auto; 
    }

