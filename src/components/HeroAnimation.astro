        3% { opacity: 1; transform: translateY(0) rotateX(0deg) rotateY(0deg); }
        11% { opacity: 1; transform: translateY(0) rotateZ(2deg); }
        14% { opacity: 0; transform: translateY(-40px) rotateX(-20deg); }
        100% { opacity: 0; }
    }

    @keyframes sequenceSearchActive {
        0%, 14% { opacity: 0; transform: scale(0.8) rotateX(20deg); }
        17% { opacity: 1; transform: scale(1) rotateX(0deg); }
        25% { opacity: 1; transform: scale(1) rotateY(5deg); } 
        28% { opacity: 0; transform: scale(1.1) rotateX(-10deg); }
        100% { opacity: 0; }
    }

    @keyframes sequenceCity {
        0%, 28% { opacity: 0; transform: translateX(-40px) rotateY(-20deg); }
        31% { opacity: 1; transform: translateX(0) rotateY(0deg); }
        39% { opacity: 1; transform: rotateZ(-2deg); }
        42% { opacity: 0; transform: translateY(-30px) rotateX(10deg); }
        100% { opacity: 0; }
    }
    @keyframes sliderFillAnim {
        0%, 31% { width: 0%; }
        37% { width: 60%; } 
        100% { width: 60%; }
    }
    @keyframes sliderThumbAnim {
        0%, 31% { right: 0%; }
        37% { right: 60%; }
        100% { right: 60%; }
    }

    @keyframes sequenceResults {
        0%, 42% { opacity: 0; transform: translateY(100px) rotateX(-20deg) scale(0.9); background-position: center top; }
        45% { opacity: 1; transform: translateY(0) rotateX(0deg) scale(1); }
        /* Scroll deeper */
        53% { background-position: center -220px; transform: rotateY(5deg); } 
        /* Hold longer for profile view */
        58% { opacity: 1; transform: translateZ(0); }
